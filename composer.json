{
    "name": "sajadsoft1/biometric-device",
    "description": "Event-driven Laravel package for biometric device communication with DTO support. Supports WebSocket, TCP, and other protocols for attendance and access control systems.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "biometric",
        "attendance",
        "aiface",
        "websocket",
        "fingerprint",
        "face-recognition",
        "access-control",
        "dto",
        "event-driven"
    ],
    "authors": [
        {
            "name": "Sajadsoft",
            "email": "info@sajadsoft.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/events": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Sajadsoft\\BiometricDevices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sajadsoft\\BiometricDevices\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sajadsoft\\BiometricDevices\\BiometricDevicesServiceProvider"
            ],
            "aliases": {
                "BiometricDevice": "Sajadsoft\\BiometricDevices\\Facades\\BiometricDevice"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

